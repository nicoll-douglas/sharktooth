{
  "name": "sharktooth",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:electron\"",
    "dev:frontend": "react-router dev",
    "watch:electron": "tsc -w -p tsconfig.electron.json",
    "dev:electron": "concurrently \"npm:watch:electron\" \"wait-on http://localhost:5173 && nodemon\"",
    "build:electron": "tsc -p tsconfig.electron.json",
    "start:electron": "electron dist/electron/main.js"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.26.0",
    "@emotion/react": "^11.14.0",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@tanstack/react-query": "^5.86.0",
    "dotenv": "^17.2.2",
    "electron-conf": "^1.3.0",
    "isbot": "^5.1.27",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.7.1",
    "socket.io-client": "^4.8.1",
    "usehooks-ts": "^3.1.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.7.1",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "electron": "^37.3.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wait-on": "^8.0.4"
  }
}
